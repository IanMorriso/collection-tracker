{
  "name": "web-scraper-app",
  "version": "1.0.0",
  "description": "Web scraping application with TypeScript, React, and MongoDB",
  "scripts": {
    "start": "ts-node src/server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon src/server/index.ts",
    "client": "cd client && npm start",
    "build": "tsc"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/cors": "^2.8.13",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  }
}
